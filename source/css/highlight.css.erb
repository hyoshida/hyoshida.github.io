<%= Rouge::Themes::Monokai.render(:scope => '.highlight') %>

.highlight {
  margin-bottom: 10.5px;
  background-color: #333;
  border-radius: 4px;
}

.highlight pre {
  color: inherit;
  background-color: inherit;
  border: inherit;
  font-size: inherit;
}

.highlight pre.lineno {
  color: #75715e;
}

.highlight span.k {
  color: #F92672;
}
